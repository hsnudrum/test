<h1>Listing discount_codes</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Price</th>
    <th>Expiration</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @discount_codes.each do |discount_code| %>
  <tr>
    <td><%= discount_code.code %></td>
    <td><%= discount_code.price %></td>
    <td><%= discount_code.expiration %></td>
    <td><%= link_to 'Show', discount_code %></td>
    <td><%= link_to 'Edit', edit_discount_code_path(discount_code) %></td>
    <td><%= link_to 'Destroy', discount_code, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Discount code', new_discount_code_path %>
