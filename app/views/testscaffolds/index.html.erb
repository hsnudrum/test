<%- model_class = Testscaffold -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:mystringxx) %></th>
      <th><%= model_class.human_attribute_name(:mytextxx) %></th>
      <th><%= model_class.human_attribute_name(:myintegerxx) %></th>
      <th><%= model_class.human_attribute_name(:myfloatxx) %></th>
      <th><%= model_class.human_attribute_name(:mydecimalxx) %></th>
      <th><%= model_class.human_attribute_name(:mydatetimexx) %></th>
      <th><%= model_class.human_attribute_name(:mytimestampxx) %></th>
      <th><%= model_class.human_attribute_name(:mytimexx) %></th>
      <th><%= model_class.human_attribute_name(:mydatexx) %></th>
      <th><%= model_class.human_attribute_name(:mybinaryxx) %></th>
      <th><%= model_class.human_attribute_name(:mybooleanxx) %></th>
      <th><%= model_class.human_attribute_name(:myreferencesxx_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @testscaffolds.each do |testscaffold| %>
      <tr>
        <td><%= link_to testscaffold.id, testscaffold_path(testscaffold) %></td>
        <td><%= testscaffold.mystringxx %></td>
        <td><%= testscaffold.mytextxx %></td>
        <td><%= testscaffold.myintegerxx %></td>
        <td><%= testscaffold.myfloatxx %></td>
        <td><%= testscaffold.mydecimalxx %></td>
        <td><%= testscaffold.mydatetimexx %></td>
        <td><%= testscaffold.mytimestampxx %></td>
        <td><%= testscaffold.mytimexx %></td>
        <td><%= testscaffold.mydatexx %></td>
        <td><%= testscaffold.mybinaryxx %></td>
        <td><%= testscaffold.mybooleanxx %></td>
        <td><%= testscaffold.myreferencesxx_id %></td>
        <td><%=l testscaffold.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_testscaffold_path(testscaffold), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      testscaffold_path(testscaffold),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_testscaffold_path,
            :class => 'btn btn-primary' %>
